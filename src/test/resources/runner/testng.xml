<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Data Driven Basics">

<groups>
   <run>
     <include name="Sanity"/>
     <exclude name="Regressionss"/>
     
   </run>
</groups>


<listeners>
   <listener class-name="com.w2a.utilities.ExtentReportManager"/>
</listeners>


  <test name="LoginTest">
    <classes>
      <class name="com.w2a.testcases.TestCases"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
